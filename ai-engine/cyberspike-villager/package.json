{
  "name": "@apollo/cyberspike-villager",
  "version": "0.1.0",
  "description": "AI-Native C2 Framework - World's First AI-Driven Penetration Testing System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'core/**/*.ts' 'modules/**/*.ts'",
    "format": "prettier --write 'core/**/*.ts' 'modules/**/*.ts'",
    "api": "python3 api/fastapi-server.py",
    "docker:kali": "docker build -t apollo/kali-container docker/kali-container",
    "docker:browser": "docker build -t apollo/browser-automation docker/browser-automation"
  },
  "keywords": [
    "ai",
    "c2",
    "penetration-testing",
    "security",
    "autonomous",
    "cybersecurity",
    "apollo"
  ],
  "author": "Apollo Platform",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "fastify": "^4.25.0",
    "socket.io": "^4.6.0",
    "dockerode": "^4.0.0",
    "ssh2": "^1.14.0",
    "playwright": "^1.40.0",
    "@anthropic-ai/sdk": "^0.9.0",
    "openai": "^4.20.0",
    "@google/generative-ai": "^0.1.0",
    "yaml": "^2.3.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0",
    "crypto-js": "^4.2.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-platform/cyberspike-villager.git"
  },
  "bugs": {
    "url": "https://github.com/apollo-platform/cyberspike-villager/issues"
  }
}
